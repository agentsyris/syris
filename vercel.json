{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/maintenance-control",
      "dest": "/maintenance-control.html"
    },
    {
      "src": "/protected-access",
      "dest": "/protected-access.html"
    }
  ],
  "env": {
    "MAINTENANCE_MODE": "@maintenance_mode"
  },
  "functions": {
    "index.html": {
      "runtime": "nodejs18.x"
    }
  }
}
